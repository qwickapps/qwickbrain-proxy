{
  "name": "@qwickapps/qwickbrain-proxy",
  "version": "1.0.0",
  "description": "Local MCP proxy for QwickBrain with caching and resilience",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "qwickbrain-proxy": "dist/bin/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/bin/cli.ts serve",
    "start": "node dist/bin/cli.js serve",
    "test": "vitest",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "qwickbrain",
    "proxy",
    "cache",
    "ai",
    "code-analysis"
  ],
  "author": "QwickApps <legal@qwickapps.com>",
  "license": "PolyForm-Shield-1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwickapps/qwickbrain-proxy.git"
  },
  "homepage": "https://github.com/qwickapps/qwickbrain-proxy#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^11.0.0",
    "commander": "^12.0.0",
    "drizzle-orm": "^0.36.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
